<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mapper.MydictMapper">

    <select id="selectByEng" parameterType="String" resultType="mydict">
        select *
        from mydict
        where word = #{eng};
    </select>

</mapper>